nts":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":"1 creature"},"range":{"value":"120 feet"},"area":{"value":null,"areaType":""},"time":{"value":"1 to 3"},"duration":{"value":""},"damage":{"value":{"0":{"type":{"categories":[],"value":"force"},"value":"1d4+1"}}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"UMUZPXnEIJEMJbfm","heightenedLevel":1},"hasCounteractCheck":{"value":false},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""}},"ownership":{"default":0,"bWA8khYkEWvh0G5V":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"OxWO0saN26IC2QYo","name":"Read Aura","type":"spell","img":"systems/pf2e/icons/spells/read-aura.webp","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.OhD2Z6rIGGD5ocZA"}},"system":{"description":{"value":"<p>You focus on the target object, opening your mind to perceive magical auras. When the casting is complete, you know whether that item is magical, and if it is, you learn the school of magic.</p>\n<p>If the object is illusory, you detect this only if the effect's level is lower than the level of your read aura spell.</p>\n<hr />\n<p><strong>Heightened (3rd)</strong> You can target up to 10 objects.</p>\n<p><strong>Heightened (6th)</strong> You can target any number of objects.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["detection","cantrip"],"rarity":"common","custom":""},"rules":[],"slug":"read-aura","schema":{"version":0.788,"lastMigration":{"datetime":"2022-09-18T12:08:38.784-04:00","version":{"schema":0.769,"foundry":"10.285","system":"4.0.6"}}},"level":{"value":1},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["arcane","divine","occult","primal"],"custom":""},"school":{"value":"divination"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":"1 object"},"range":{"value":"30 feet"},"area":{"value":null,"areaType":""},"time":{"value":"1 Minute"},"duration":{"value":""},"damage":{"value":{}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"UMUZPXnEIJEMJbfm"},"hasCounteractCheck":{"value":false},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightening":{"levels":{"3":{"target":{"value":"10 objects"}},"6":{"target":{"value":"any number of objects"}}},"type":"fixed"}},"ownership":{"default":0,"bWA8khYkEWvh0G5V":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"jC5IvuYK3vtFoTCC","name":"Sigil","type":"spell","img":"systems/pf2e/icons/spells/sigil.webp","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.AUctDF2fqPZN2w4W"}},"system":{"description":{"value":"<p>You harmlessly place your unique magical sigil, which is about 1 square inch in size, on the targeted creature or object. The mark can be visible or invisible, and you can change it from one state to another by using an Interact action to touch the target. The mark can be scrubbed or scraped off with 5 minutes of work. If it's on a creature, it fades naturally over the course of a week. The time before the mark fades increases depending on your heightened level.</p>\n<hr />\n<p><strong>Heightened (3rd)</strong> The sigil instead fades after 1 month.</p>\n<p><strong>Heightened (5th)</strong> The sigil instead fades after 1 year.</p>\n<p><strong>Heightened (7th)</strong> The sigil never fades.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["cantrip"],"rarity":"common","custom":""},"rules":[],"slug":"sigil","schema":{"version":0.788,"lastMigration":{"datetime":"2022-09-18T12:08:38.784-04:00","version":{"schema":0.769,"foundry":"10.285","system":"4.0.6"}}},"level":{"value":1},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["arcane","divine","occult","primal"],"custom":""},"school":{"value":"transmutation"},"components":{"focus":false,"material":false,"somatic":false,"verbal":false},"materials":{"value":""},"target":{"value":"1 creature or object"},"range":{"value":"touch"},"area":{"value":null,"areaType":""},"time":{"value":"2"},"duration":{"value":"unlimited"},"damage":{"value":{}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"UMUZPXnEIJEMJbfm"},"hasCounteractCheck":{"value":false},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""}},"ownership":{"default":0,"bWA8khYkEWvh0G5V":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}}],"effects":[],"folder":null,"sort":0,"flags":{"pf2e":{"lootable":false},"core":{"sourceId":"Compendium.battlezoo-bestiary-pf2e.pf2e-battlezoo-bestiary.wclFBoscXaBK7cLP"}},"system":{"attributes":{"hp":{"value":20,"temp":0,"max":20,"details":""},"initiative":{"ability":"perception"},"speed":{"value":15,"otherSpeeds":[{"type":"fly","label":"Fly","value":15}],"details":""},"ac":{"value":14,"details":"","base":14},"perception":{"value":7},"allSaves":{"value":""}},"details":{"alignment":{"value":"N"},"level":{"value":1},"source":{"value":"Battlezoo Bestiary","author":"Douglas Edwards"},"creatureType":"","blurb":"","publicNotes":"<p>Alchemists have been known to spend countless hours and days working on new formulae in order to expand their repertoire of elixirs and explore the frontiers of alchemical knowledge. Under certain conditions, this devotion to the study of math, science, and transmutation can take on a life all its own. When a potentially powerful recipe or formula is left on the crafting table half- finished, it can manifest the will to pick up where its creator left off. Overnight, the composition feverishly works itself into completion and beyond, forming an incomprehensible proof that has a mind of its own. Alchemists have come to lovingly call these mathematically animated pages living proofs.</p>\n<p>Physically, living proofs are usually composed of nothing more than parchment and ink. They mobilize by folding and re-folding themselves like origami. Though this talent allows living proofs to take on just about any shape, they tend to choose forms that resemble miniature versions of their creators or, rarely, some other creature important to the proof. For example, a living proof written in snake's blood might take the guise of a papery serpent. Similarly, a living proof whose first sight upon animating is that of a caged specimen such as a chimera or owlbear might model itself after such a monster.</p>\n<p>Although most living proofs dedicate themselves to one particular area of study, the occasional specimen might become a polymath if they were designed by a particularly genius alchemist. If abandoned or forgotten, a living proof may wander from its laboratory to seek a new master to inspire and aid, though they are otherwise loyal assistants. Sometimes this loyalty goes a bit too far&mdash;more than one alchemist apprentice has wound up injured after dawdling in their master's lab after hours and stumbling upon a seemingly mundane sheet of parchment come to life.</p>\n<h2>Unusual Materials</h2>\n<p>Living proofs made of unusual materials&mdash;such as vellum, stone tablets, or even chalkboards&mdash;are not unknown. Such specimens typically have different resistances and weaknesses depending on the material they are made of.</p>\n<h2>Monster Part Subsystem</h2>\n<p><strong>Living Proof Creature 1 </strong></p>\n<p><strong>Monster Parts</strong> 3.5/7/13 gp</p>\n<p><strong>Eligible Refinements</strong> weapons (piercing, slashing), armor (light), perception item, skill items (Acrobatics, Arcana, Crafting)</p>\n<p><strong>Eligible Imbued Properties</strong> bane, dexterity, energy resistant (negative, positive), force, intelligence, sensory (darkvision), spells (Arcane: magic missile, read aura, sigil), wild, winged</p>","privateNotes":""},"traits":{"value":["construct","minion"],"senses":{"value":"darkvision"},"languages":{"value":["common"],"selected":[],"custom":"(Can't speak any language)"},"di":{"value":["bleed","death-effects","disease","doomed","drained","fatigued","healing","necromancy","nonlethal-attacks","paralyzed","poison","sickened","unconscious"],"custom":""},"dr":[],"dv":[{"type":"fire","value":3},{"type":"slashing","value":3}],"rarity":"common","size":{"value":"tiny"},"ci":[]},"resources":{},"schema":{"version":0.788,"lastMigration":{"datetime":"2022-09-18T12:08:38.784-04:00","version":{"schema":0.769,"foundry":"10.285","system":"4.0.6"}}},"abilities":{"str":{"mod":-4},"dex":{"mod":4},"con":{"mod":0},"int":{"mod":5},"wis":{"mod":1},"cha":{"mod":-5}},"saves":{"fortitude":{"value":4,"saveDetail":""},"reflex":{"value":10,"saveDetail":""},"will":{"value":6,"saveDetail":""}},"customModifiers":{}},"ownership":{"default":0,"bWA8khYkEWvh0G5V":3},"prototypeToken":{"bar1":{"attribute":"attributes.hp"},"displayName":20,"displayBars":20,"flags":{"pf2e":{"linkToActorSize":true}},"height":0.5,"width":0.5,"disposition":-1,"name":"Living Proof","actorLink":false,"lockRotation":false,"rotation":0,"alpha":1,"bar2":{"attribute":null},"randomImg":false,"light":{"alpha":0.25,"angle":360,"bright":0,"coloration":1,"dim":0,"luminosity":0.5,"saturation":0,"contrast":0,"shadows":0,"animation":{"speed":5,"intensity":5,"type":null,"reverse":false},"darkness":{"min":0,"max":1},"attenuation":0.5,"color":null},"texture":{"src":"modules/battlezoo-best