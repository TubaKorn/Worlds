["concentrate","occult","polymorph","transmutation"],"rarity":"common","custom":""},"rules":[],"slug":"change-shape","schema":{"version":0.788,"lastMigration":{"datetime":"2022-09-18T12:08:34.967-04:00","version":{"schema":0.769,"foundry":"10.285","system":"4.0.6"}}},"actionType":{"value":"action"},"actionCategory":{"value":"offensive"},"actions":{"value":1},"requirements":{"value":""},"trigger":{"value":""},"deathNote":false,"weapon":{"value":""}},"ownership":{"default":0,"bWA8khYkEWvh0G5V":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"bxUJ7qU9yl29HXKf","name":"Leech Mind","type":"action","img":"systems/pf2e/icons/actions/TwoActions.webp","effects":[],"folder":null,"sort":0,"flags":{},"system":{"description":{"value":"<p data-visibility=\"gm\"><strong>Requirements</strong> The myzoform has a creature @Compendium[pf2e.conditionitems.Grabbed]{Grabbed};</p>\n<p><strong>Effect</strong> The myzoform latches its many lamprey mouths onto the grabbed creature, then feasts upon the victim's memories, dealing [[/r {4d4+10}[mental]]]{4d4+10 mental damage}. The target must attempt a @Check[type:will|dc:25|name:Leach Mind DC|traits:damaging-effect, mental, necromancy, occult|showDC:gm]{Will} save.</p>\n<div data-visibility=\"gm\"><hr />\n<p><strong>Critical Success</strong> The creature takes no damage and the grab ends.</p>\n<p><strong>Success</strong> The creature takes half damage.</p>\n<p><strong>Failure</strong> The creature takes full damage, is @Compendium[pf2e.conditionitems.Stupefied]{Stupefied 1}, and permanently loses 1 minute's worth of memories, which are replaced with only an indistinct haze. The stupefied condition remains until the creature receives magical healing.</p>\n<p><strong>Critical Failure</strong> As failure, but the creature takes double damage, is @Compendium[pf2e.conditionitems.Stupefied]{Stupefied 2}, and loses 5 minutes' worth of memories.</p>\n</div>"},"source":{"value":""},"traits":{"value":["mental","necromancy","occult"],"rarity":"common","custom":""},"rules":[],"slug":null,"schema":{"version":0.788,"lastMigration":{"datetime":"2022-09-18T12:08:34.967-04:00","version":{"schema":0.769,"foundry":"10.285","system":"4.0.6"}}},"actionType":{"value":"action"},"actionCategory":{"value":"offensive"},"actions":{"value":2},"requirements":{"value":""},"trigger":{"value":""},"deathNote":false},"ownership":{"default":0,"bWA8khYkEWvh0G5V":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"hnOTjY1e5zwDiA53","name":"Darkvision","type":"action","img":"systems/pf2e/icons/actions/Passive.webp","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.pf2e.bestiary-ability-glossary-srd.qCCLZhnp2HhP3Ex6"}},"system":{"description":{"value":"<p>@Localize[PF2E.NPC.Abilities.Glossary.Darkvision]</p>"},"source":{"value":""},"traits":{"value":[],"rarity":"common","custom":""},"rules":[],"slug":"darkvision","schema":{"version":0.788,"lastMigration":{"datetime":"2022-09-18T12:08:34.967-04:00","version":{"schema":0.769,"foundry":"10.285","system":"4.0.6"}}},"actionType":{"value":"passive"},"actionCategory":{"value":"interaction"},"actions":{"value":null},"requirements":{"value":""},"trigger":{"value":""},"deathNote":false,"weapon":{"value":""}},"ownership":{"default":0,"bWA8khYkEWvh0G5V":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"tBKcXBhVAwskLVn8","name":"Swarm Mind","type":"action","img":"systems/pf2e/icons/actions/Passive.webp","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.pf2e.bestiary-ability-glossary-srd.baA0nSMhQyFyJIia"}},"system":{"description":{"value":"<p>@Localize[PF2E.NPC.Abilities.Glossary.SwarmMind]</p>"},"source":{"value":""},"traits":{"value":[],"rarity":"common","custom":""},"rules":[],"slug":"swarm-mind","schema":{"version":0.788,"lastMigration":{"datetime":"2022-09-18T12:08:34.967-04:00","version":{"schema":0.769,"foundry":"10.285","system":"4.0.6"}}},"actionType":{"value":"passive"},"actionCategory":{"value":"defensive"},"actions":{"value":null},"requirements":{"value":""},"trigger":{"value":""},"deathNote":false,"weapon":{"value":""}},"ownership":{"default":0,"ldigRFmLUDwvP1vo":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"wOBkrBkkDKS3Gf9C","name":"Athletics","type":"lore","img":"systems/pf2e/icons/default-icons/lore.svg","effects":[],"folder":null,"sort":0,"flags":{},"system":{"description":{"value":""},"source":{"value":""},"traits":{"value":[],"rarity":"common","custom":""},"rules":[],"slug":null,"schema":{"version":0.788,"lastMigration":{"datetime":"2022-09-18T12:08:34.967-04:00","version":{"schema":0.769,"foundry":"10.285","system":"4.0.6"}}},"mod":{"value":17},"proficient":{"value":0}},"ownership":{"default":0,"ldigRFmLUDwvP1vo":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}}],"effects":[],"folder":null,"sort":0,"flags":{"pf2e":{"lootable":false},"core":{"sourceId":"Compendium.battlezoo.pf2e-battlezoo-bestiary.1GexAsGe5Y8XgXSZ"}},"system":{"attributes":{"hp":{"value":130,"temp":0,"max":130,"details":""},"initiative":{"ability":"perception"},"speed":{"value":25,"otherSpeeds":[],"details":""},"ac":{"value":26,"details":"","base":26},"perception":{"value":16},"allSaves":{"value":""}},"details":{"alignment":{"value":"NE"},"level":{"value":8},"source":{"value":"Battlezoo Bestiary","author":"Bella Kelley"},"creatureType":"","blurb":"","publicNotes":"<p>A myzoform is an amalgam of magically modified lampreys fused together into a single humanoid shape. Its head is a writhing mass of lampreys which collectively act as the creature's eyes, ears, and mouths. Myzoforms come about when a mundane lamprey is accidentally exposed to strange and powerful magics&mdash;be it from the monstrous aboleths, wild sorcerers, or even natural sources. As a result of its exposure, the lamprey becomes pre-sapient, allowing it to feed on and digest creatures' memories as easily as a normal lamprey sucks blood.</p>\n<p>When such a pre-sapient lamprey attaches itself to another pre-sapient lamprey, the two create a kind of feedback loop as their minds interconnect and feed on and regurgitate one another's increasingly broken and bizarre memories. The lampreys then look for more lampreys to turn into their body, and the process snowballs until a myzoform is born.</p>\n<p>At this stage, a myzoform can survive on its own experiences and memories, but most myzoforms find themselves addicted to the \"fresh\" memories of other creatures. These thirsty creatures are often found haunting towns, cities, and other urban centers rich with potential prey on whose minds they can absorb. When a myzoform strikes from the shadows and latches onto the mind of an unfortunate local, they absorb everything&mdash;personality, memories, knowledge&mdash;and leave in their wake only an amnesiac.</p>\n<p>The most gluttonous and malicious myzoforms have fed on countless minds throughout their lives, and they might contain hundreds or even thousands of consumed personalities. As the number of minds inside them increases, only the most powerful myzoforms can stay in control; most eventually break down and self-destruct as their collective minds become a civil war of different fears, ideologies, and motivations</p>\n<h2>Crisis of Consciences</h2>\n<p>Although some myzoforms can be genuinely malicious, those are rare. Many myzoforms feel highly guilty over their nigh unstoppable impulse to feed on other sapient beings. Some rationalize their behavior by saying they can't control their impulses, while others might try to cure themselves of their \"curse\" and seek outside help to overcome their insatiable addiction.</p>\n<h2>Monster Part Subsystem</h2>\n<p><strong>Myzoform Creature 8 </strong></p>\n<p><strong>Monster Parts</strong> 64/140/250 gp</p>\n<p><strong>Eligible Refinements</strong> weapons (bludgeoning), armor (light), perception item, skill items (Arcana, Athletics, Deception, Occultism, Stealth)</p>\n<p><strong>Eligible Imbued Properties</strong> acid, bane, chaotic, constitution, energy resistant (positive), evil, force, intelligence, mental, sensory (darkvision), spells (Occult: acid arrow, blindness, confusion, darkness, magic missile, tongues), wild</p>","privateNotes":""},"traits":{"value":["aberration","amphibious","swarm"],"senses":{"value":"darkvision"},"languages":{"value":[],"selected":[],"custom":"Tongues"},"di":{"value":["grabbed","prone","restrained","swarm-mind"],"custom":""},"dr":[],"dv":[],"rarity":"common","size":{"value":"lg"},"ci":[]},"resources":{},"schema":{"version":0.788,"lastMigration":{"datetime":"2022-09-18T12:08:34.966-04:00","version":{"schema":0.769,"foundry":"10.285","system":"4.0.6"}}},"abilities":{"str":{"mod":3},"dex":{"mod":4},"con":{"mod":5},"int":{"mod":6},"wis":{"mod":2},"cha":{"mod":0}},"saves":{"fortitude":{"value":19,"saveDetail":""},"reflex":{"value":16,"saveDetail":""},"will":{"value":14,"saveDetail":""}}},"ownership":{"default":0,"ldigRFmLUDwvP1vo":3},"prototypeToken":{"bar1":{"attribute":"attributes.hp"},"displayName":20,"displayBars":20,"flags":{"pf2e":{"linkToActorSize":true}},"height":2,"width":2,"disposition":-1,"name":"Myzoform","actorLink":false,"lockRotation":false,"rotation":0,"alpha":1,"bar2":{"attribute":null},"randomImg":false,"light":{"alpha":0.25,"angle":360,"bright":0,"coloration":1,"dim":0,"luminosity":0.5,"saturation":0,"contrast":0,"shadows":0,"animation":{"speed":5,"intensity":5,"type":null,"reverse":false},"darkness":{"min":0,"max":1},"color":null,"attenuation":0.5},"texture":{"src":"modules/battlezoo-bestiary-pf2e/art/Tokens/myzoform_token.webp","tint":null,"scaleX":1,"scaleY":1,"offsetX":0,"offsetY":0,"rotation":0},"sight":{"angle":360,"enabled":false,"range":0,"brightness":1,"visionMode":"basic","color":null,"attenuation":0.1,"saturation":0,"contrast":0},"detectionModes":[]},"_stats":{"systemId":"pf2e","systemVersion":"4.0.6","coreVersion":"10.285","createdTime":1663517316833,"modifiedTime":1663517460586,"lastModifiedBy":"IWDF0nA8gEs5avF1"}}
