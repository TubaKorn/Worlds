S18]{Sickened 2}.</p>\n<hr />\n<p><strong>Heightened (+1)</strong> The damage increases by 2d4 and the Persistent Damage by 1d4.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["mental","nonlethal"],"rarity":"common","custom":""},"rules":[],"slug":"phantom-pain","schema":{"version":0.788,"lastMigration":{"datetime":"2022-09-18T12:08:38.576-04:00","version":{"schema":0.769,"foundry":"10.285","system":"4.0.6"}}},"level":{"value":1},"spellType":{"value":"save"},"category":{"value":"spell"},"traditions":{"value":["occult"]},"school":{"value":"illusion"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":"1 creature"},"range":{"value":"30 feet"},"area":{"value":"","areaType":""},"time":{"value":"2"},"duration":{"value":"1 minute"},"damage":{"value":{"0":{"type":{"categories":[],"value":"mental","subtype":""},"value":"2d4","applyMod":false}}},"save":{"value":"will","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"xK2wxZVBRUrun3dd","heightenedLevel":2},"hasCounteractCheck":{"value":false},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightening":{"type":"interval","interval":1,"damage":{"0":"2d4"}}},"ownership":{"default":0,"bWA8khYkEWvh0G5V":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"0oAWSApLB0ZZNGbA","name":"Ritual","type":"spellcastingEntry","img":"systems/pf2e/icons/default-icons/spellcastingEntry.svg","effects":[],"folder":null,"sort":0,"flags":{},"system":{"description":{"value":""},"source":{"value":""},"traits":{"value":[],"rarity":"common","custom":""},"rules":[],"slug":null,"schema":{"version":0.788,"lastMigration":{"datetime":"2022-09-18T12:08:38.576-04:00","version":{"schema":0.769,"foundry":"10.285","system":"4.0.6"}}},"ability":{