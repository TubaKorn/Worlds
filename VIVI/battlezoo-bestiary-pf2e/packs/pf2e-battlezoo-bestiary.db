etime":"2022-09-18T12:08:35.605-04:00","version":{"schema":0.769,"foundry":"10.285","system":"4.0.6"}}},"level":{"value":1},"spellType":{"value":"save"},"category":{"value":"spell"},"traditions":{"value":["primal"]},"school":{"value":"abjuration"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":""},"area":{"value":null,"areaType":""},"time":{"value":"2"},"duration":{"value":"1 hour"},"damage":{"value":{}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"3AFj8qkJtWx2k7BY","heightenedLevel":2},"hasCounteractCheck":{"value":false},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightening":{"levels":{"4":{"area":{"areaType":"emanation","value":"20"},"range":{"value":"20 feet"}}},"type":"fixed"}},"ownership":{"default":0,"bWA8khYkEWvh0G5V":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"5cPJGtnKKvrAsgrq","name":"Jump","type":"spell","img":"systems/pf2e/icons/spells/jump.webp","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.Q7QQ91vQtyi1Ux36"}},"system":{"description":{"value":"<p>Your legs surge with strength, ready to leap high and far. You jump 30 feet in any direction without touching the ground. You must land on a space of solid ground within 30 feet of you, or else you fall after using your next action.</p>\n<hr />\n<p><strong>Heightened (3rd)</strong> The range becomes touch, the target changes to one touched creature, and the duration becomes 1 minute, allowing the target to jump as described whenever it takes the Leap action.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["move"],"rarity":"common","custom":""},"rules":[],"slug":"jump","schema":{"version":0.788,"lastMigration":{"datetime":"2022-09-18T12:08:35.605-04:00","version":{"schema":0.769,"foundry":"10.285","system":"4.0.6"}}},"level":{"value":1},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["arcane","primal"]},"school":{"value":"transmutation"},"components":{"focus":false,"material":false,"somatic":true,"verbal":false},"materials":{"value":""},"target":{"value":""},"range":{"value":""},"area":{"value":null,"areaType":""},"time":{"value":"1"},"duration":{"value":""},"damage":{"value":{}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"3AFj8qkJtWx2k7BY","heightenedLevel":3},"hasCounteractCheck":{"value":false},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightening":{"levels":{"3":{"range":{"value":"touch"},"target":{"value":"1 creature"}}},"type":"fixed"}},"ownership":{"default":0,"bWA8khYkEWvh0G5V":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"yzmP4hWELfeYAVwJ","name":"Equip Liege","type":"action","img":"systems/pf2e/icons/actions/OneAction.webp","effects":[],"folder":null,"sort":400000,"flags":{},"system":{"description":{"value":"<p><span class=\"pf2-icon\">a</span> to <span class=\"pf2-icon\">2</span></p>\n<p data-visibility=\"gm\"><strong>Requirements</strong> The trodaiche-sidhe is adjacent to a willing ally;</p>\n<p><strong>Effect</strong> The trodaiche-sidhe deftly draws an item of 1 Bulk or less and hands it off to their ally with supernatural speed. The trodaiche-sidhe places the item their ally's free hand or, if no hand is free, among their ally's gear. This ability takes 1 action if the item is of Light or negligible Bulk, or 2 actions if it is of 1 Bulk.</p>"},"source":{"value":""},"traits":{"value":[],"rarity":"common","custom":""},"rules":[],"slug":null,"schema":{"version":0.788,"lastMigration":{"datetime":"2022-09-18T12:08:35.605-04:00","version":{"schema":0.769,"foundry":"10.285","system":"4.0.6"}}},"actionType":{"value":"action"},"actionCategory":{"value":"offensive"},"actions":{"value":null},"requirements":{"value":""},"trigger":{"value":""},"deathNote":false},"ownership":{"default":0,"bWA8khYkEWvh0G5V":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"QzfIp6uJOEyg7GRB","name":"Suppressive Volley","type":"action","img":"systems/pf2e/icons/actions/TwoActions.webp","effects":[],"folder":null,"sort":500000,"flags":{},"system":{"description":{"value":"<p>The trodaiche-sidhe rapidly fires a hail of 5 arrows at a target within 100 feet, forcing it to move away or be assailed by the salvo. The target takes [[/r {3d8 + 6}[piercing]]]{3d8 + 6 piercing damage} (@Check[type:reflex|dc:25|name:Suppressive Volley DC|traits:damaging-effect|showDC:gm]{basic Reflex} save). After rolling its save, the target can move 5 feet as a reaction to increase its save's degree of success by one step (from failure to success, for example). The creature must be able to spend a reaction to do this, and this movement triggers reactions as normal.</p>"},"source":{"value":""},"traits":{"value":[],"rarity":"common","custom":""},"rules":[],"slug":null,"schema":{"version":0.788,"lastMigration":{"datetime":"2022-09-18T12:08:35.605-04:00","version":{"schema":0.769,"foundry":"10.285","system":"4.0.6"}}},"actionType":{"value":"action"},"actionCategory":{"value":"offensive"},"actions":{"value":2},"requirements":{"value":""},"trigger":{"value":""},"deathNote":false},"ownership":{"default":0,"bWA8khYkEWvh0G5V":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}}],"effects":[],"folder":null,"sort":0,"flags":{"pf2e":{"lootable":false},"core":{"sourceId":"Compendium.battlezoo-bestiary-pf2e.pf2e-battlezoo-bestiary.fHjw75uyKYE3vGGT"}},"system":{"attributes":{"hp":{"value":90,"temp":0,"max":90,"details":""},"initiative":{"ability":"perception"},"speed":{"value":30,"otherSpeeds":[],"details":""},"ac":{"value":24,"details":"","base":24},"perception":{"value":16},"allSaves":{"value":""}},"details":{"alignment":{"value":"CN"},"level":{"value":6},"source":{"value":"Battlezoo Bestiary","author":"Adam Kessler"},"creatureType":"","blurb":"","publicNotes":"<p>Often mistaken for incredibly beautiful elves, trodaiche-sidhes are a type of fey that participate in the wild hunt. With supernatural grace and lithe features, trodaiche-sidhes may be among the weakest of the wild hunt's supernatural stalkers and monster slayers, but they are still deadly in their own right, especially when encountered alongside their older and stronger kin. They serve as magnificent aides and attach&eacute;s, deftly taking care of all the things that make a successful hunt possible. Duties range from sharpening weapons and oiling armor, crafting arrows, flushing game from bushes, and any other tasks that allow their superiors to focus entirely upon the hunt. Most of them do this only because they have been promised they will one day be invited to join the wild hunt proper, though how this process works&mdash;or how long a trodaiche-sidhe must endure being a servant&mdash;is not known.</p>\n<p>Trodaiche-sidhes practice with all manner of arms and armor, though they are fond of elven weapons in particular. Even unarmed, a trodaiche-sidhe is more than a match for most mortals.</p>\n<h2>The Wild Hunt</h2>\n<p>The wild hunt is a group of enigmatic fey who hunt prey across not only the fey realm, but across the entire multiverse. Among the wild hunt, trodaiche-sidhes are considered highly reliable squires.</p>\n<h2>Monster Part Subsystem</h2>\n<p><strong>Trodaiche-Sidhe Creature 6 </strong></p>\n<p><strong>Monster Parts</strong> 30/65/125 gp</p>\n<p><strong>Eligible Refinements</strong> weapons (bludgeoning), armor (light, medium), skill items (Acrobatics, Athletics, Crafting, Lore [Hunting], Nature, Stealth, Survival)</p>\n<p><strong>Eligible Imbued Properties</strong> bane, chaotic, charisma, dexterity, energy resistant (positive), sensory (low-light vision), spells (Primal: glitterdust, jump, pass without trace), strength, wild, wisdom</p>","privateNotes":""},"traits":{"value":["fey","hb_wild-hunt"],"senses":{"value":"low-light vision"},"languages":{"value":["common","sylvan"],"selected":[],"custom":""},"di":{"value":[],"custom":""},"dr":[],"dv":[{"type":"coldiron","value":7}],"rarity":"common","size":{"value":"med"},"ci":[]},"resources":{},"schema":{"version":0.788,"lastMigration":{"datetime":"2022-09-18T12:08:35.605-04:00","version":{"schema":0.769,"foundry":"10.285","system":"4.0.6"}}},"abilities":{"str":{"mod":4},"dex":{"mod":5},"con":{"mod":3},"int":{"mod":2},"wis":{"mod":4},"cha":{"mod":5}},"saves":{"fortitude":{"value":13,"saveDetail":""},"reflex":{"value":17,"saveDetail":""},"will":{"value":12,"saveDetail":""}}},"ownership":{"default":0,"bWA8khYkEWvh0G5V":3},"prototypeToken":{"bar1":{"attribute":"attributes.hp"},"displayName":20,"displayBars":20,"flags":{"pf2e":{"linkToActorSize":true}},"height":1,"width":1,"disposition":-1,"name":"Trodaiche-Sidhe","actorLink":false,"lockRotation":false,"rotation":0,"alpha":1,"bar2":{"attribute":null},"randomImg":false,"light":{"alpha":0.25,"angle":360,"bright":0,"coloration":1,"dim":0,"luminosity":0.5,"saturation":0,"contrast":0,"shadows":0,"animation":{"speed":5,"intensity":5,"type":null,"reverse":false},"darkness":{"min":0,"max":1},"attenuation":0.5,"color":null},"texture":{"src":"modules/battlezoo-bestiary-pf2e/art/Tokens/trodaiche-sidhe_token.webp","scaleX":1,"scaleY":1,"offsetX":0,"offsetY":0,"rotation":0,"tint":null},"sight":{"angle":360,"enabled":false,"range":0,"brightness":1,"visionMode":"basic","color":null,"attenuation":0.1,"saturation":0,"contrast":0},"detectionModes":[]},"_stats":{"systemId":"